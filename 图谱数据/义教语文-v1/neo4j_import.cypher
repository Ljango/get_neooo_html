MERGE (n:CoreLiteracy {identifier: "urn:jy:chinese:SB0101:OB02,OB03:CoreLiteracy:l3"})
SET n += {type: "CoreLiteracy", title: "思维能力", description: "思维能力是指学生在语文学习过程中的联想想象、分析比较、归纳判断等认知表现，主要包括直觉思维、形象思维、逻辑思维、辩证思维和创造思维。思维具有一定的敏捷性、灵活性、深刻性、独创性、批判性。有好奇心、求知欲，崇尚真知，勇于探索创新，养成积极思考的习惯。", subject: "SB0101", applicableLevel: "[\"OB02\", \"OB03\"]", contentJson: "{}", identifier: "urn:jy:chinese:SB0101:OB02,OB03:CoreLiteracy:l3"};

