MERGE (n:GoalPoint {identifier: "urn:jy:English:SB0102:OB06:GoalPoint:1"})
SET n += {identifier: "urn:jy:English:SB0102:OB06:GoalPoint:1", title: "内容1", description: "", subject: "SB0102", type: "GoalPoint", applicableLevel: "OB06", contentJson: "{\"text\": \"能在常见的具体语境中整合性地运用语言知识、技能和策略，理解语篇所表达的意义，识别其恰当表意所采用的手段，具备一定的语言意识和英语语感，进行有意义的表达与交流。\"}", CJ_text: "能在常见的具体语境中整合性地运用语言知识、技能和策略，理解语篇所表达的意义，识别其恰当表意所采用的手段，具备一定的语言意识和英语语感，进行有意义的表达与交流。"};

MERGE (n:GoalPoint {identifier: "urn:jy:English:SB0102:OB06:GoalPoint:2"})
